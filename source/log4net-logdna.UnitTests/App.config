<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <root>
      <level value="ALL" />
      <appender-ref ref="LogdnaAppender" />
    </root>
    <appender name="LogdnaAppender" type="log4net.logdna.LogdnaAppender, log4net-logdna">
      <rootUrl value="https://fake.logs-01.loggly.com/" />
      <inputKey value="your-customer-token" />
      <tag value="your-custom-tag" />
      <sendInterval value="00:00:00.01" />
    </appender>
  </log4net>
</configuration>