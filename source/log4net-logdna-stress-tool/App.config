<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <root>
      <level value="ALL" />
      <appender-ref ref="LogdnaAppender" />
    </root>
    <appender name="LogdnaAppender" type="log4net.Logdna.LogdnaAppender, log4net-logdna">
      <rootUrl value="https://logs.logdna.com/" />
      <inputKey value="APIKey" />
      <tag value="log4net-test-logdna" />
      <app value="Log4net-logdna" />
      <env value="prod" />
      <numberOfInnerExceptions value="4"/>
      <!--<layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC1}] - %message%newline" />
      </layout>-->
    </appender>
  </log4net>
</configuration>